{
    "manifest_version": 2,
    "name": "Pierce Launcher",
    "description": "Quick launch Pierce Media",
    "version": "1.0.0",
    "icons": {"128": "icon_128.PNG"},
    "browser_action": {
        "default_icon": "icon.PNG",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab"]
}
